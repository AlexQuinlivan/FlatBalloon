{
  "name": "Helium",
  "version": "0.0.1",
  "summary": "An Android-like layout inflation and bucketed resource library.",
  "homepage": "https://github.com/AlexQuinlivan/Helium",
  "license": "Apache License, Version 2.0",
  "authors": {
    "Alex Quinlivan": "alex@quinlivan.co.nz"
  },
  "source": {
    "git": "https://github.com/AlexQuinlivan/Helium.git",
    "tag": "0.0.1"
  },
  "social_media_url": "https://twitter.com/quin1212",
  "platforms": {
    "ios": "7.0"
  },
  "requires_arc": true,
  "source_files": "Pod/Classes/**/*",
  "exclude_files": "Pod/Classes/**/GDataXMLNode.{h,m}",
  "dependencies": {
    "EDSemver": [
      "~> 0.3.0"
    ]
  },
  "libraries": "xml2",
  "xcconfig": {
    "HEADER_SEARCH_PATHS": "$(SDKROOT)/usr/include/libxml2"
  },
  "subspecs": [
    {
      "name": "no-arc",
      "source_files": "Pod/Classes/GDataXMLNode.{h,m}",
      "requires_arc": false
    }
  ]
}
